{
	"config": {
		"prefix": "/v1",
		"status": "ACTIVE"
	},
	"routes": [
		{
			"requestUri": "/user/fetchAll",
			"httpMethod": "get",
			"handler": "/v1/MyController.fetchAllUsers",
			"validationRules": {
				"cookies.asgardTheme": {
					"presence": true
				}
			}			
		},
		{
			"requestUri": "/user/renderAll",
			"httpMethod": "get",
			"handler": "/v1/MyController.renderAllUsers"
		},
		{
			"requestUri" : "/user/addCountry",
			"httpMethod" : "post",
			"handler" : "/v1/MyController.addCountry"
		},
		{
			"requestUri" : "/user/getCountry",
			"httpMethod" : "get",
			"handler" : "/v1/MyController.getCountry", 
			"cacheEnabled" : true
		}			
	]
}
